<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
    <navigation-rule>
    <from-view-id>/calculator.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>success</from-outcome>
      <to-view-id>/result.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
   <display-name>listarPersonas.xhtml</display-name>
   <from-view-id>/listarPersonas.xhtml</from-view-id>
   <navigation-case>
       <from-outcome>actualiza</from-outcome>
       <to-view-id>/editarPersona.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
       <from-outcome>recarga</from-outcome>
       <to-view-id>/listarPersonas.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
   <display-name>editarPersona.xhtml</display-name>
   <from-view-id>/editarPersona.xhtml</from-view-id>
   <navigation-case>
       <from-outcome>success</from-outcome>
       <to-view-id>/listarPersonas.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
   <display-name>crearPersona.xhtml</display-name>
   <from-view-id>/crearPersona.xhtml</from-view-id>
   <navigation-case>
       <from-outcome>success</from-outcome>
       <to-view-id>/listarPersonas.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
</faces-config>
